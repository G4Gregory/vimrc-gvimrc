:syntax enable

"fontsize
set guifont=Menlo\ Regular:h17

"window dimensions
set lines=30
set columns=100

"transparency set...
set transparency=10

"setting line break
set tw=85

"display invisibles
set list
set listchars=tab:▸\ ,eol:¬

	"invisible character colors
	highlight NonText guifg=#4a4a59
	highlight NonText guibg=NONE
	highlight SpecialKey guifg=#4a4a59

"show tabs at all times
set stal=2
